{% if navigationParent %}
  <script type="application/ld+json">
    {
      "@context": "https://schema.org",
      "@type": "BreadcrumbList",
      "itemListElement": [
        {
          "@type": "ListItem",
          "position": 1,
          "name": "{{ navigationParent.name }}",
          "item": "{{ env.siteUrl }}/{{ navigationParent.url }}"
        }
      ]
    }
  </script>
{% endif %}
